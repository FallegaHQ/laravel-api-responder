{
    "name": "fallegahq/laravel-api-responder",
    "description": "A comprehensive Laravel package for building consistent API responses with DTOs, caching, and role-based visibility",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "FallegaHQ\\ApiResponder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FallegaHQ\\ApiResponder\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "SAKHRAOUI Omar",
            "email": "softwyx@softwyx.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "illuminate/pagination": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/validation": "^10.0|^11.0|^12.0",
        "illuminate/auth": "^10.0|^11.0|^12.0",
        "illuminate/cache": "^10.0|^11.0|^12.0",
        "illuminate/events": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "FallegaHQ\\ApiResponder\\ApiResponderServiceProvider"
            ]
        }
    }
}
